{
  "name": "lto",
  "version": "0.3.0",
  "license": "MIT",
  "type": "module",
  "bin": {
    "lto": "./index.js"
  },
  "packageManager": "pnpm@10.26.2",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "release": "pnpm run build && publint && changeset publish",
    "typecheck": "tsc --noEmit",
    "lint": "eslint",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.1",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@types/node": "^24.10.4",
    "esbuild": "^0.27.2",
    "eslint": "^9.39.2",
    "eslint-config-mado": "^0.11.0",
    "globals": "^16.5.0",
    "prettier": "^3.7.4",
    "publint": "^0.3.16",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "author": {
    "email": "doinki94@gmail.com",
    "name": "doinki",
    "url": "https://github.com/doinki"
  },
  "bugs": "https://github.com/doinki/lto/issues",
  "files": [
    "dist",
    "index.js"
  ],
  "homepage": "https://github.com/doinki/lto",
  "keywords": [
    "lto"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/doinki/lto.git"
  },
  "publishConfig": {
    "access": "public"
  }
}
